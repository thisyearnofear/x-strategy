{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "lib": [
            "es2020"
        ],
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "generated": [
                "./generated"
            ]
        },
        "skipLibCheck": true
    },
    "include": [
        "src/**/*",
        "generated/**/*"
    ]
}